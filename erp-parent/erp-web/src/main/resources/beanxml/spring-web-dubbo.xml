    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:dubbo="http://dubbo.apache.org/schema/dubbo"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    	http://www.springframework.org/schema/beans/spring-beans-4.3.xsd
    	http://dubbo.apache.org/schema/dubbo
    	http://dubbo.apache.org/schema/dubbo/dubbo.xsd">

    <dubbo:reference id="userDelegate" interface="org.layz.hx.erp.domain.delegate.system.UserDelegate" check="false"/>
    <dubbo:reference id="systemErpDelegate" interface="org.layz.hx.erp.domain.delegate.system.SystemErpDelegate" check="false"/>
    <dubbo:reference id="roleDelegate" interface="org.layz.hx.erp.domain.delegate.system.RoleDelegate" check="false"/>
    <dubbo:reference id="jobDelegate" interface="org.layz.hx.erp.domain.delegate.system.ErpJobDelegate" check="false"/>
    <dubbo:reference id="menuDelegate" interface="org.layz.hx.erp.domain.delegate.system.MenuDelegate" check="false"/>
</beans>